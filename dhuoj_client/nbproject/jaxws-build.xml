<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:j2seproject3="http://www.netbeans.org/ns/j2se-project/3" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/client"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${libs.jaxws21.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-UserAccountService" depends="wsimport-init">
        <property name="wsdl-UserAccountService" location="xml-resources/web-service-references/UserAccountService/wsdl/acm.dhu.edu.cn_80/dhuoj/UserAccountService.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/client" destdir="${build.generated.dir}/wsimport/client" wsdl="${wsdl-UserAccountService}" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="http://acm.dhu.edu.cn:80/dhuoj/UserAccountService?wsdl" xnocompile="true">
            <produces dir="${build.generated.dir}/wsimport/client/cn/edu/dhu/acm/oj/webservice" includes="UserAccountService.java" casesensitive="no"/>
        </wsimport>
    </target>
    <target name="wsimport-client-clean-UserAccountService" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/client/cn/edu/dhu/acm/oj/webservice"/>
    </target>
    <target name="wsimport-client-ContestService" depends="wsimport-init">
        <property name="wsdl-ContestService" location="xml-resources/web-service-references/ContestService/wsdl/acm.dhu.edu.cn_80/dhuoj/ContestService.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/client" destdir="${build.generated.dir}/wsimport/client" wsdl="${wsdl-ContestService}" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="http://acm.dhu.edu.cn:80/dhuoj/ContestService?wsdl" xnocompile="true">
            <produces dir="${build.generated.dir}/wsimport/client/cn/edu/dhu/acm/oj/webservice" includes="ContestService.java" casesensitive="no"/>
        </wsimport>
    </target>
    <target name="wsimport-client-clean-ContestService" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/client/cn/edu/dhu/acm/oj/webservice"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-UserAccountService, wsimport-client-ContestService"/>
    <target name="wsimport-client-compile-depend" if="do.depend.true">
        <j2seproject3:depend srcdir="${build.generated.dir}/wsimport/client" classpath="${libs.jaxws21.classpath}:${javac.classpath}" destdir="${build.classes.dir}"/>
    </target>
    <target name="wsimport-client-compile" depends="-pre-pre-compile, wsimport-client-compile-depend">
        <j2seproject3:javac srcdir="${build.generated.dir}/wsimport/client" classpath="${libs.jaxws21.classpath}:${javac.classpath}" destdir="${build.classes.dir}"/>
        <copy todir="${build.classes.dir}">
            <fileset dir="${build.generated.dir}/wsimport/client" includes="**/*.xml"/>
        </copy>
    </target>
</project>
